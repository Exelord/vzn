{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/build/(.js)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public,max-age=31536000,immutable"
        }
      ]
    }
  ]
}